{
  "extends": "airbnb",
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "jsx": true
  },
  "plugins": [
    "typescript"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "object-curly-newline": "off",
    "lines-between-class-members": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-param-reassign": "off",
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off"
  }
}
